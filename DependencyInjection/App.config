<?xml version="1.0" encoding="utf-8" ?>
<configuration>

  <configSections>
    <section name="unity" type="Microsoft.Practices.Unity.Configuration.UnityConfigurationSection, Unity.Configuration"/>
  </configSections>
  <unity xmlns="http://schemas.microsoft.com/practices/2010/unity">
    <alias alias="IDraw" type="DependencyInjection.IDraw, DependencyInjection"/>
    <alias alias="MainClass" type="DependencyInjection.MainClass, DependencyInjection"/>
    <alias alias="DrawSquare" type="DependencyInjection.DrawSquare, DependencyInjection"/>
    
    <namespace name="DependencyInjection"/>
    <assembly name="DependencyInjection"/>
      <container>
       
        <register type="IDraw" mapTo="DrawSquare" />
        <register type="MainClass"/>
      </container>
  </unity>
  
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
</configuration>